            "accordion_row_6xAybE": {
              "type": "_accordion-row",
              "settings": {
                "heading": "What condition will my ReWair product be in?",
                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_KfTgdM": {
                  "type": "text",
                  "settings": {
                    "text": "<p>We have three grades of footwear: ‘Excellent’, ‘Good’ and 'Fair'. All ReWair silhouettes are cleaned and sanitised inside and out to a professional standard using Micro-Fresh®.</p>",
                    "width": "100%",
                    "max_width": "none",
                    "alignment": "left",
                    "type_preset": "rte",
                    "font": "var(--font-body--family)",
                    "font_size": "1rem",
